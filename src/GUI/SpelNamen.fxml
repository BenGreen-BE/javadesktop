<fx:root orientation="VERTICAL" stylesheets="@hoofdpaneel.css" type="FlowPane" vgap="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
    <VBox prefHeight="390.0" prefWidth="156.0" BorderPane.alignment="CENTER">
        <children>
            <ListView fx:id="lvwLijstSpelNamen" onMouseClicked="#lvwSpelNamenOnMouseClicked" prefHeight="210.0" prefWidth="184.0">
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin></ListView>
        </children>
    </VBox>
    <VBox prefHeight="386.0" prefWidth="440.0" BorderPane.alignment="CENTER">
        <children>
            <Label fx:id="lblOverzicht" prefHeight="0.0" prefWidth="711.0">
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin></Label>
            <ImageView fx:id="imgOverzicht" fitHeight="187.0" fitWidth="330.0" pickOnBounds="true" preserveRatio="true">
                <VBox.margin>
                    <Insets bottom="10.0" left="105.0" right="10.0" top="10.0" />
                </VBox.margin>
               <effect>
                  <DropShadow />
               </effect>
            </ImageView>
            <Label fx:id="lblDesc" prefHeight="26.0" prefWidth="442.0" />
            <!-- <Label fx:id="lblMoeilijkheid" prefHeight="30.0" prefWidth="440.0" /> -->
            <Label fx:id="lblAantalSpelborden" prefHeight="31.0" prefWidth="440.0" />
            <HBox prefHeight="64.0" prefWidth="436.0">
                <children>
                    <Button fx:id="btnPlaySpelMenu" mnemonicParsing="false" onMouseClicked="#btnPlaySpelMenuOnMouseClicked" prefHeight="54.0" prefWidth="178.0">
                        <HBox.margin>
                            <Insets left="25.0" right="10.0" />
                        </HBox.margin>
                    </Button>
                    <Button fx:id="btnBackSpelMenu" mnemonicParsing="false" onMouseClicked="#btnBackSpelMenuOnMouseClicked" prefHeight="54.0" prefWidth="185.0">
                        <HBox.margin>
                            <Insets left="10.0" right="25.0" />
                        </HBox.margin>
                    </Button>
                </children>
                <VBox.margin>
                    <Insets top="15.0" />
                </VBox.margin>
            </HBox>
        </children>
         <FlowPane.margin>
            <Insets left="75.0" />
         </FlowPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
    </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="55.0" right="25.0" top="50.0" />
   </padding>
</fx:root>
